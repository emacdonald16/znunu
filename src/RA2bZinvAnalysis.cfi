# tree path = root://cmseos.fnal.gov//store/user/lpcsusyhad/SusyRA2Analysis2015/Skims/Run2ProductionV16/  # xrootd
# tree path = /eos/uscms/store/user/lpcsusyhad/SusyRA2Analysis2015/Skims/Run2ProductionV16/  # from cmslpc
root file index = ../datFiles/skimFilesV12.dat
delta phi sample = nominal  # ldp, hdp
integrated luminosity = 35.9
apply Z/gamma Pt cut = true
